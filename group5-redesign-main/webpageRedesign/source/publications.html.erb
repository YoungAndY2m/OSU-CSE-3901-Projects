---
title: Publications
---

<div class="publications">
    <div>
        <table style = "width:100%">
            <caption>Publications</caption>
            <tr>
                <th>Title</th>
                <th>Event</th>
                <th>Authors</th>
                <th>Date</th>
                <th>Link</th>
            </tr>
            <% data.publications.publications.each do |pub| %>
                <tr class="tr-data">
                    <td><%= pub["title"]%></td>
                    <td><%= pub["event"]%></td>
                    <td><%= pub["author"]%></td>
                    <td><%= pub["date"]%></td>
                    <td>
                        <% if pub["link"].length != 0 %>
                            <a href= "<%= pub["link"]%>">View</a>
                        <% elsif pub["link"].length == 0 %>
                            <%= "N/A" %>
                        <% end %>
                    </td>
                </tr>
            <% end %>
        </table>
    </div>

    <hr>

    <div>
        <table style = "width:100%">
            <caption>Conference and Journal Articles</caption>
            <tr>
                <th>Title</th>
                <th>Event</th>
                <th>Authors</th>
                <th>Date</th>
                <th>Link</th>
            </tr>
            <% data.publications.conference_journal.each do |cj| %>
                <tr class="tr-data">
                    <td><%= cj["title"]%></td>
